[Unit]
Description=PkamSmartHome Actor
After=network-online.target mosquitto.service

[Service]
Type=simple
NotifyAccess=main
ExecStart=/home/openhabian/PkamSmartHome/psh-actor/start.sh
# Restart=always
# RestartSec=1
User=openhabian
WorkingDirectory=/home/openhabian/PkamSmartHome/psh-actor/


[Install]
WantedBy=multi-user.target
